{
  "manifest_version": 3,
  "name": "ATG LinkedIn Scraper",
  "version": "1.0",
  "description": "Automatically scrape LinkedIn profile details and send to backend API.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "assets/icon.png"
  },
  "icons": {
    "16": "assets/icon.png",
    "48": "assets/icon.png",
    "128": "assets/icon.png"
  },
  "permissions": ["tabs", "scripting", "activeTab"]
}
